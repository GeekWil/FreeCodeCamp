function convertToRoman(num) {
 const romanNum = {
    1000: 'M',
    900: 'CM',
    500: 'D',
    400: 'CD',
    100: 'C',
    90: 'XC',
    50: 'L',
    40: 'XL',
    10: 'X',
    9: 'IX',
    5: 'V',
    4: 'IV',
    1: 'I'
  };

  let result = '';

  for (let value in romanNum) {
    while (num >= value) {
      result += romanNum[value];
      num -= value;
    }
  }

  return result;

}

convertToRoman(36);
